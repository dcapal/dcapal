  "scripts": {
    "build": "webpack --config webpack.prod.js --mode production",
    "build-dev": "webpack --config webpack.prod.js --mode development",
    "check": "npx prettier --check 'src/**/*.{js,ts}'",
    "format": "npx prettier --write 'src/**/*.{js,ts}'",
