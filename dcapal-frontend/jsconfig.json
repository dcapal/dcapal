{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@app/*": [
        "src/app/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@demo/*": [
        "demo/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@images/*": [
        "images/*"
      ],
      "@routes/*": [
        "src/routes/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@workers/*": [
        "src/workers/*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}